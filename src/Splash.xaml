<Window x:Class="SPV3.Splash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Foreground="White"
        Title="SPV3 Loading..." Height="337" Width="600" WindowStyle="None" AllowsTransparency="True">
    <Window.Effect>
        <DropShadowEffect BlurRadius="24" Opacity="0.9" Direction="275"/>
    </Window.Effect>
    <Window.Background>
        <ImageBrush ImageSource="Assets/spv3_art.png"/>
    </Window.Background>
    <Grid>
        <Label Foreground="White">Artwork By @Dagger6art</Label>
    </Grid>
</Window>
